<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.honey.magistro">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Student_List" android:theme="@style/AppTheme"></activity>
        <activity android:name=".StudentInfo" android:theme="@style/AppTheme"></activity>
        <activity android:name=".AddStaff"></activity>
        <activity android:name=".AddInstitute"></activity>
        <activity android:name=".AddCourse"></activity>
        <activity android:name=".AddBatches"></activity>
    </application>

</manifest>